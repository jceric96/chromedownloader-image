{
	"name": "Chrome Downloader",
	"description": "Extension to Download images from Google Images",
	"version": "1.1",
	"manifest_version": 3,

	"icons": {
		"128":"icon_128.png"
	},
	"background":{
		"service_worker": "background.js",
		"type": "module"
	},
	"action": {
		"default_popup": "popup.html"
	},

	"permissions": [
		"activeTab",
		"storage",
		"downloads",
		"scripting"
	],
	"host_permissions":[
		"<all_urls>"
	]

	
}